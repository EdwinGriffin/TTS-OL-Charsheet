      class="AtkName"
      width="197"
      height="49"
      textAlignment="MiddleLeft"/>

    <InputField
      class="AtkMod"
      width="96"
      height="49"
      characterLimit="3"
      characterValidation="Integer"
      textAlignment="MiddleCenter"/>

    <InputField
      class="AtkDmg"
      width="191"
      height="49"
      textAlignment="MiddleCenter"/>

    <InputField
      class="AtksSpells"
      width="535"
      height="32"
      textAlignment="LowerLeft"/>

    <!--EQUIPMENT-->
    <InputField
      class="Currency"
      width="88"
      height="48"
      characterValidation="Integer"
      textAlignment="MiddleCenter"/>

    <InputField
      class="Equipment"
      width="194"
      height="32"
      textAlignment="LowerLeft"/>

    <!--IDEALS, BONDS & FLAWS-->
    <InputField
      class="Personality"
      width="490"
      height="162"
      lineType="MultiLineNewLine"
      resizeTextMaxSize="24"
      textAlignment="UpperLeft"/>

    <InputField
      class="IdealBondFlaw"
      width="490"
      height="119"
      lineType="MultiLineNewLine"
      resizeTextMaxSize="24"
      textAlignment="UpperLeft"/>

    <!--FEATURES-->
