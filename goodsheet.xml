<Defaults>
    <!-- Styling for the reset button -->
    <Button
      fontStyle="Bold"
      horizontalOverflow="Wrap"
      resizeTextForBestFit="true"
      resizeTextMinSize="12"
      resizeTextMaxSize="200"/>
    <!-- This determines the styling for text boxes (classified as Input Fields). Colour is a key entry as it prevents the emissive in low light issue -->
    <InputField
      onEndEdit="updateField"
      colors="#FFFFFF00|#FFFFFF00|#FFFFFF00|#FFFFFF00"
      fontStyle="Bold"
      horizontalOverflow="Wrap"
      placeholder=" "
      resizeTextForBestFit="true"
      resizeTextMinSize="12"
      resizeTextMaxSize="200"/>
    <!-- General text styling -->
    <Text
      fontStyle="Bold"
      horizontalOverflow="Wrap"
      resizeTextForBestFit="true"
      resizeTextMinSize="12"
      resizeTextMaxSize="200"/>
    <!-- Styling for the lock button -->
    <ToggleButton
      fontStyle="Bold"
      horizontalOverflow="Wrap"
      resizeTextForBestFit="true"
      resizeTextMinSize="12"
      resizeTextMaxSize="200"/>

    <!--INPUTFIELD TYPES, defining classes for different kinds of text boxes-->
    <InputField class="ALC" textAlignment="LowerCenter"/>
    <InputField class="AMC" textAlignment="MiddleCenter"/>
    <InputField class="N" characterValidation="Integer"/>
    <InputField class="L2" characterLimit="2"/>

    <!--TOGGLEBUTTON TYPES, defining the styling for the click button bullet things -->
    <ToggleButton
      class="Bullet"
      onValueChanged="updateBullet"
      color="#FFFFFF00"
      horizontalOverflow="Overflow"
      textAlignment="MiddleCenter"
      verticalOverflow="Overflow"/>

    <!--SHEET-->
    <Panel
      class="Sheet"
      position="0 0 -20.1"
      rotation="180 180 0"
      scale="0.08235 0.08235"
      width="1878"
      height="2429"/>

    <!--BASIC INFO-->
    <InputField
      class="ClassSubPlayer"
      width="283"
      height="60"
      textAlignment="LowerLeft"/>

    <InputField
      class="RaceBGAlign"
      width="237"
      height="60"
      textAlignment="LowerLeft"/>

    <!--ATTRIBUTES-->
    <InputField
      class="AttrMod"
      width="129"
      height="88"
      characterLimit="2"
      characterValidation="Integer"
      textAlignment="MiddleCenter"/>

    <InputField
      class="Attr"
      width="67"
      height="54"
      characterLimit="2"
      characterValidation="Integer"
      textAlignment="MiddleCenter"/>

    <!--SAVING THROWS & SKILLS-->
    <ToggleButton
      class="Prof"
      width="21"
      height="21"
      fontSize="64"/>

    <ToggleButton
      class="Exp"
      width="16"
      height="16"
      fontSize="54"/>

    <InputField
      class="ProfMod"
      width="45"
      height="39"
      characterLimit="2"
      characterValidation="Integer"
      textAlignment="LowerCenter"/>

    <!--OTHER PROFICIENCIES-->
    <InputField
      class="OtherProfs"
      width="526"
      height="32"
      textAlignment="LowerLeft"/>

    <!--ARMOR & SHIELDS-->
    <InputField
      class="ArmorName"
      width="359"
      height="36"
      textAlignment="MiddleLeft"/>

    <InputField
      class="Armor"
      width="96"
      height="36"
      characterLimit="2"
      characterValidation="Integer"
      textAlignment="MiddleCenter"/>

    <!--HIT POINTS-->
    <InputField
      class="Hp"
      width="158"
      height="103"
      characterValidation="Integer"
      textAlignment="MiddleCenter"/>

    <!--HIT DICE-->
    <InputField
      class="HitDiceTotal"
      width="40"
      height="32"
      characterLimit="2"
      characterValidation="Integer"
      textAlignment="LowerCenter"/>

    <InputField
      class="HitDice"
      width="140"
      height="32"
      textAlignment="LowerCenter"/>

    <!--DEATH SAVES-->
